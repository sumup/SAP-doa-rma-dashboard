    {
      "name": "zdoa_rma_300",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": [
        "run-script",
        "start"
      ],
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "args": [
        "--",
        "--ui5Uri https://ui5.sap.com --ui5 1.84.6",
        "--backendConfig",
        "[{\"path\":\"/sap\",\"destination\":\"DEV300\"}] "
      ],
      "windows": {
        "runtimeExecutable": "npm.cmd",
        "args": [
          "--",
          "--ui5Uri https://ui5.sap.com --ui5 1.84.6",
          "--backendConfig",
          "\"[{\\\"path\\\":\\\"/sap\\\",\\\"destination\\\":\\\"DEV300\\\"}]\" "
        ]
      },
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "env": {
        "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/zdoa_rma\"}"
      }
    }
  ],
  "inputs": [
    {
      "id": "UI5Version",
      "type": "command",
      "command": "sap.ux.appGenerator.ui5Version"
    }
  ]
